@model Alumno;

@{
    ViewData["Title"] = "Información Alumno";
}

<div>
    <h1>@ViewData["Title"]</h1>
    <div>
        <strong>Vista Normal</strong>
        <div>
            <p>
                Id: @Model.Id
                <br>
                Nombre: @Model.Nombre
                <br>
                Id Curso: @Model.CursoId
            </p>
        </div>
    </div>
    <div>
        <strong>Vista parcial</strong>
        <partial name="_ObjetoEscuelaSimple" />
    </div>
</div>
